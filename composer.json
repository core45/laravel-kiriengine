{
    "name": "core45/laravel-kiriengine",
    "description": "Laravel KIRI Engine API integration",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Core45\\LaravelKiriengine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core45\\LaravelKiriengine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core45\\LaravelKiriengine\\KiriengineServiceProvider"
            ],
            "aliases": {
                "Kiriengine": "Core45\\LaravelKiriengine\\Facades\\Kiriengine"
            }
        }
    },
    "authors": [
        {
            "name": "Kornel",
            "email": "kornel@core45.com"
        }
    ],
    "minimum-stability": "dev",
    "version": "1.1.7",
    "scripts": {
        "test": "vendor/bin/pest"
    }
}

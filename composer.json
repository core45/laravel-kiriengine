{
    "name": "core45/laravel-kiriengine",
    "description": "A Laravel package for interacting with the Kiriengine API",
    "type": "library",
    "license": "MIT",
    "version": "1.0.6",
    "autoload": {
        "psr-4": {
            "Core45\\LaravelKiriengine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Core45",
            "email": "info@core45.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core45\\LaravelKiriengine\\KiriengineServiceProvider"
            ],
            "aliases": {
                "Kiriengine": "Core45\\LaravelKiriengine\\Facades\\Kiriengine"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
